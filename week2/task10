class UserMainCode
{
    public int sumOfSumsOfDigits(int input1)
    {
String s = String.valueOf(input1);
        int totalSum = 0;

        for (int i = 0; i < s.length(); i++)
        {
            int sum = 0;

            for (int j = i; j < s.length(); j++)
            {
                sum += s.charAt(j) - '0';
            }

            totalSum += sum;
        }

        return totalSum;
   }
}
